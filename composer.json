{
  "name": "mimmi20/browscap-php-unsorted",
  "type": "library",
  "description": "Standalone replacement for php's native get_browser() function",
  "keywords": [
    "get_browser",
    "browser",
    "capabilities",
    "user agent"
  ],
  "homepage": "https://github.com/mimmi20/browscap-unsorted",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas Mueller",
      "email": "t_mueller_stolzenhain@yahoo.de"
    }
  ],
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.2.2",
    "friendsofphp/php-cs-fixer": "^2.16.1",
    "phpunit/phpunit": "^7.5.20 || ^8.5.0"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-0": {
      "phpbrowscap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "phpbrowscapTest\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/browscap-unsorted/issues",
    "source": "https://github.com/mimmi20/browscap-unsorted"
  }
}
