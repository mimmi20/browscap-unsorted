{
  "name": "mimmi20/browscap-php-unsorted",
  "type": "library",
  "description": "Standalone replacement for php's native get_browser() function",
  "keywords": [
    "get_browser",
    "browser",
    "capabilities",
    "user agent"
  ],
  "homepage": "https://github.com/mimmi20/browscap-unsorted",
  "license": "MIT",
  "authors": [
    {
      "name": "Jonathan Stoppani",
      "email": "jonathan.stoppani@gmail.com"
    },
    {
      "name": "James Titcumb",
      "email": "james@asgrim.com"
    },
    {
      "name": "Thomas Mueller",
      "email": "t_mueller_stolzenhain@yahoo.de"
    }
  ],
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16.1",
    "phpunit/phpunit": "^4.8.36 || ^5.7.26 || ^6.5.0 || ^7.5.20 || ^8.5.0"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-0": {
      "phpbrowscap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "phpbrowscapTest\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/browscap-unsorted/issues",
    "source": "https://github.com/mimmi20/browscap-unsorted"
  }
}
